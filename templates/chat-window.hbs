<section class="chat-content">
    {{! 顯示訊息的主要區塊 }}
    <ol id="custom-chat-log" class="chat-log">
        {{#each messages}}
            {{{this.html}}}
        {{/each}}
    </ol>
    {{! 回到底部的按鈕}}
    <button type="button" class="jump-to-bottom" data-action="jumpToBottom" aria-label="Jump to Bottom">
        <i class="fas fa-arrow-down"></i>
    </button>
</section>